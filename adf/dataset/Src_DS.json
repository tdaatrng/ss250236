{
	"name": "Src_DS",
	"properties": {
		"linkedServiceName": {
			"referenceName": "adls_ls",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"sourceContainer": {
				"type": "string",
				"defaultValue": "sj186068"
			},
			"sourceDirectory": {
				"type": "string",
				"defaultValue": "input"
			},
			"sourceFileName": {
				"type": "string",
				"defaultValue": "RIM_V1.2_HistoryData.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().sourceFileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().sourceDirectory",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().sourceContainer",
					"type": "Expression"
				}
			},
			"columnDelimiter": "^",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "Flight#",
				"type": "String"
			},
			{
				"name": "Origin",
				"type": "String"
			},
			{
				"name": "Dest",
				"type": "String"
			},
			{
				"name": "FlightDate",
				"type": "String"
			},
			{
				"name": "NoSalesReason",
				"type": "String"
			},
			{
				"name": "TransmittingFlightAttd",
				"type": "String"
			}
		]
	}
}