{
	"name": "Tgt_DS",
	"properties": {
		"linkedServiceName": {
			"referenceName": "adls_ls",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"File_Name_Tgt": {
				"type": "string"
			},
			"Cotainer_Name_Tgt": {
				"type": "string"
			},
			"Directory_Name_Tgt": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().File_Name_Tgt,'_',utcnow(),'.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().Directory_Name_Tgt",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().Cotainer_Name_Tgt",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	}
}