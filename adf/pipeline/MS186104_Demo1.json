{
	"name": "MS186104_Demo1",
	"properties": {
		"activities": [
			{
				"name": "CopyDataToInput",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "Inbound",
							"wildcardFileName": "Aircraft_Flown*.zip"
						},
						"formatSettings": {
							"type": "JsonReadSettings",
							"compressionProperties": {
								"type": "ZipDeflateReadSettings"
							}
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['AIRCFT_NBR']"
								},
								"sink": {
									"name": "AIRCFT_NBR"
								}
							},
							{
								"source": {
									"path": "$['ACTL_ARVL_STN']"
								},
								"sink": {
									"name": "ACTL_ARVL_STN"
								}
							},
							{
								"source": {
									"path": "$['ACTL_DEPTR_STN']"
								},
								"sink": {
									"name": "ACTL_DEPTR_STN"
								}
							},
							{
								"source": {
									"path": "$['FLIGHT_DT']"
								},
								"sink": {
									"name": "FLIGHT_DT",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['AIRCFT_FLWN_MIN']"
								},
								"sink": {
									"name": "AIRCFT_FLWN_MIN"
								}
							},
							{
								"source": {
									"path": "$['AIRCFT_FLWN_CYC']"
								},
								"sink": {
									"name": "AIRCFT_FLWN_CYC"
								}
							},
							{
								"source": {
									"path": "$['ACTL_OUT_TM']"
								},
								"sink": {
									"name": "ACTL_OUT_TM",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Aircraft_Flown_Src",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Aircraft_Flown_Data_Inp",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "RunNotebookTo_LoadData",
				"type": "DatabricksNotebook",
				"dependsOn": [
					{
						"activity": "CopyDataToInput",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/ms186104/1_Demo/2_MS186104_Demo1"
				},
				"linkedServiceName": {
					"referenceName": "adb_ls",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "File_Archive",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "RunNotebookTo_LoadData",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "MS186104_Demo1_File_Archive",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			}
		],
		"folder": {
			"name": "MS186104"
		},
		"annotations": []
	}
}