{
	"name": "ah186033_demo",
	"properties": {
		"activities": [
			{
				"name": "Demo_databricks_notebook",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/ah186033/sample"
				},
				"linkedServiceName": {
					"referenceName": "adb_ls",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "Success",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Demo_databricks_notebook",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "ah186033_email",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"Activity": "Email Success",
						"EmailTo": {
							"value": "abdul.hakimuddin@teradata.com",
							"type": "Expression"
						},
						"Error_Message": {
							"value": "N/A",
							"type": "Expression"
						},
						"Pipe_Line_Name": {
							"value": "@pipeline().Pipeline",
							"type": "Expression"
						},
						"Source_File_Name": {
							"value": "N/A",
							"type": "Expression"
						},
						"Subject": {
							"value": "@(pipeline().Pipeline Executed Successfully)",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "Failure",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Demo_databricks_notebook",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "ah186033_email",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"Activity": {
							"value": "Email Failure",
							"type": "Expression"
						},
						"EmailTo": {
							"value": "abdul.hakimuddin@teradata.com",
							"type": "Expression"
						},
						"Error_Message": {
							"value": "\"@pipeline().Pipeline.Failed\"",
							"type": "Expression"
						},
						"Pipe_Line_Name": {
							"value": "@pipeline().Pipeline",
							"type": "Expression"
						},
						"Source_File_Name": "N/A",
						"Subject": {
							"value": "\"@pipeline().Pipeline.Failed\"",
							"type": "Expression"
						}
					}
				}
			}
		],
		"annotations": []
	}
}