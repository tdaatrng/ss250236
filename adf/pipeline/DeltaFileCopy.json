{
	"name": "DeltaFileCopy",
	"properties": {
		"activities": [
			{
				"name": "SetRunTimestamp",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "RunTimeStamp",
					"value": {
						"value": "@utcnow('yyyyMMddHHmmss')",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set Data File Pattern",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "DataFilePattern",
					"value": {
						"value": "@concat(pipeline().parameters.FilePattern,'*.dat.gz')",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"EmailTo": {
				"type": "string",
				"defaultValue": "Jayadeep.Patil@Teradata.com"
			},
			"FilePattern": {
				"type": "string",
				"defaultValue": "TEST_FILE"
			},
			"SubjectAreaName": {
				"type": "string",
				"defaultValue": "abc"
			}
		},
		"variables": {
			"CTLFileName": {
				"type": "String"
			},
			"DataFileName": {
				"type": "String"
			},
			"RunTimeStamp": {
				"type": "String"
			},
			"DataFilePattern": {
				"type": "String"
			},
			"CTLFilePattern": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Take2"
		},
		"annotations": []
	}
}