{
	"name": "Demo_Pipeline",
	"properties": {
		"activities": [
			{
				"name": "ExtractFileName",
				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "CSVFileName",
					"value": {
						"value": "@substring(pipeline().parameters.FileName,0,lastindexof(pipeline().parameters.FileName,'.'))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "ExtractCTLFileName",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "ExtractFileName",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "CTLFileName",
					"value": {
						"value": "@substring(variables('CSVFileName'),0,lastindexof(variables('CSVFileName'),'.'))",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "ABC.dat.gz"
			},
			"ContainerName": {
				"type": "string"
			},
			"InputDir": {
				"type": "string"
			},
			"InboundDir": {
				"type": "string"
			}
		},
		"variables": {
			"CSVFileName": {
				"type": "String"
			},
			"CTLFileName": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Take2"
		},
		"annotations": []
	}
}