{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "SrcToWrk",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['WindowEnd']"
								},
								"sink": {
									"name": "WindowEnd",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['FraudulentCalls']"
								},
								"sink": {
									"name": "FraudulentCalls",
									"type": "Int64"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "StreamData",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "TgtData",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"FileSys": {
				"type": "string",
				"defaultValue": "ss250236"
			},
			"Dir": {
				"type": "string",
				"defaultValue": "IN"
			},
			"TgtFileNm": {
				"type": "string",
				"defaultValue": "TGT"
			},
			"SrcFileNm": {
				"type": "string",
				"defaultValue": "RIM"
			}
		},
		"annotations": []
	}
}