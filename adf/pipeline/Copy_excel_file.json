{
	"name": "Copy_excel_file",
	"properties": {
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						}
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "EmpID",
									"type": "String"
								},
								"sink": {
									"path": "$['Column_1']"
								}
							},
							{
								"source": {
									"name": "EmpName",
									"type": "String"
								},
								"sink": {
									"path": "$['Column_2']"
								}
							},
							{
								"source": {
									"name": "Origin State",
									"type": "String"
								},
								"sink": {
									"path": "$['Column_3']"
								}
							},
							{
								"source": {
									"name": "Current State",
									"type": "String"
								},
								"sink": {
									"path": "$['Column_4']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Excel2",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Json3",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"Current State": {
				"type": "string"
			},
			"Origin State": {
				"type": "string"
			}
		},
		"annotations": []
	}
}