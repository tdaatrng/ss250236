		"batch_adls_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'batch_adls_ls'"
		},
		"batch_blob_ln_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'batch_blob_ln'"
